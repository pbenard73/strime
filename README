1 - inside strim_node folder
    docker build --no-cache --pull -t strim_node . 
    
2 - at root
    modify the docker-compose volume entry to target the source
    docker-compose up

	
4 - browser localhost:4000 test@strime.fr:test
	create strime database at server {host: 'postgres', username:'postgres'}
